				hisilicon,hi6421-vset-table = <1500000>, <1800000>,
							      <2400000>, <2500000>,
							      <2600000>, <2700000>,
							      <2850000>, <3000000>;
				hisilicon,hi6421-off-on-delay-us = <10000>;
				hisilicon,hi6421-enable-time-us = <250>;
				hisilicon,hi6421-eco-microamp = <8000>;
			};

			ldo1: ldo@21 {
